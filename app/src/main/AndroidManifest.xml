<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.uiexample">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".BootLoaderActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".music.ui.MusicActivity" />
        <activity android:name=".custom.CustomViewActivity" />
        <activity android:name=".skip.HomePageActivity" />
        <activity android:name=".skip.TimePickerActivity" />
        <activity android:name=".skip.CommonDialogActivity" />
        <activity android:name=".skip.ContextMenuActivity" />
        <activity android:name=".others.OtherPageActivity" />
        <activity android:name=".gallery.GalleryActivity"/>
    </application>

</manifest>